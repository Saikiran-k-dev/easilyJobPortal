<div style="color: aliceblue;"><div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col" style="color: aliceblue;">#</th>
          <th scope="col" style="color: aliceblue;">Name</th>
          <th scope="col" style="color: aliceblue;">Email</th>
          <th scope="col" style="color: aliceblue;">Contact</th>
          <th scope="col" style="color: aliceblue;">Resume</th>
        </tr>
      </thead>
      <tbody>
        
        <tr>
         
         <% if(applications)  {%>
            <% applications.forEach(application=>{ %>
          <th scope="row" style="color: aliceblue;"><%=application.applicantID%></th>
         
          <td style="color: aliceblue;"><%=application.name%></td>
          <td style="color: aliceblue;"><%=application.email%></td>
          <td style="color: aliceblue;"><%=application.contact%></td>
          <td style="color: aliceblue;">
            <a class="btn btn-sm btn-primary" href="http://localhost:3200/<%=application.resume%>" target="_blank">view</a>
          </td>
        <% }) %>
       <% } %>
        </tr>
        
      </tbody>
    </table>
  </div>
  </div>